<table class="ctable" border="0" cellspacing="0" cellpadding="2">
    <tr>
        <td colspan="4" align="center">
            <b> Перекомплектации ТМЦ № {{document_number}} от {{date}}</b> <br>
        </td>
    </tr>
    <tr>
        <td>
            <b> Со склада:</b>
        </td>
        <td>
            {{from}}
        </td>

    </tr>
    <tr>
        <td>
            <b>Исходящий ТМЦ:</b>
        </td>
        <td>
            {{fromitemname}}
        </td>
    </tr>
    <tr>
        <td>
            <b> На склад:</b>
        </td>
        <td>
            {{to}}
        </td>

    </tr>
   <tr>
        <td>
            <b>Входящий ТМЦ:</b>
        </td>
        <td>
            {{toitemname}}
        </td>
    </tr>
    <tr>
        <td>
            <b>Сумма:</b>
        </td>
        <td>
            {{amount}}
        </td>
    </tr>
    <tr>
        <td colspan="2">{{{notes}}}</td>
    </tr>


</table>


